package Collectins;
import java.util.*;

public class CollectionArrList {
	public static void main(String[] args)
	{
		List<Integer> list=new ArrayList<Integer>();
		list.add(10);
		list.add(20);
		list.add(30);
		list.add(40);
		list.add(50);
		list.add(60);
		list.add(70);
		list.add(80);
		Iterator itr=list.iterator();
		while(itr.hasNext())
		{
			System.out.println(itr.next()+" ");
		}
		list.indexOf(10);
	}

}
